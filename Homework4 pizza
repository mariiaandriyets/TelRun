package Homework;

public class Pizza {
    public static void main(String[] args) {

        int r = 12;
        int q = 14;
        int w = 40;


        double d = ((Math.pow(q, 2) * Math.PI * w) - (Math.pow(r, 2) * Math.PI * w)) * Math.pow (10, -3);

        System.out.println("разница: " + d + " ккал");
    }
}
